{
  "expo": {
    "name": "ProtectiOn",
    "slug": "protection",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e9bf52"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.protection",
      "adaptiveIcon": {
        "backgroundColor": "#e9bf52"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}